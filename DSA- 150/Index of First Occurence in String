// User function Template for Java

class Solution {
    public int findMatching(String text, String pat) {
        // Code here
        int n = text.length();
        int m = pat.length();
        for(int i =0 ; i<= n-m ;i++){
         if(text.substring(i , i + m).equals(pat))  return i;
        }
        return -1;
    }
}
